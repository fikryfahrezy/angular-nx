{
  "name": "@org/source",
  "version": "0.0.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "nx run-many --disableRemoteCache -t build",
    "e2e": "nx run-many --disableRemoteCache -t e2e",
    "lint": "nx run-many --disableRemoteCache -t lint",
    "test": "nx run-many --disableRemoteCache -t test --exclude=tag:type:web",
    "typecheck": "nx run-many --disableRemoteCache -t typecheck",
    "format": "prettier --write .",
    "gen:angular-app": "nx g @nx/angular:application --bundler rspack --standalone true --addTailwind --e2eTestRunner playwright --inlineStyle false --inlineTemplate false --linter eslint --minimal true --setParserOptionsProject --strict --unitTestRunner vitest --style css --viewEncapsulation Emulated",
    "gen:angular-lib": "nx g @nx/angular:library --flat true --linter eslint --changeDetection Default --setParserOptionsProject true --standalone true --strict true --unitTestRunner vitest",
    "first:build": "nx run --disableRemoteCache first:build",
    "first:dev": "nx run --disableRemoteCache first:serve",
    "first:e2e": "nx run --disableRemoteCache first-e2e:e2e",
    "main-app:lint": "nx run --disableRemoteCache main-app:lint",
    "main-app:test": "nx run --disableRemoteCache main-app:test",
    "main-app:typecheck": "nx run --disableRemoteCache main-app:typecheck",
    "second:build": "nx run --disableRemoteCache second:build",
    "second:dev": "nx run --disableRemoteCache second:serve",
    "some-feature:lint": "nx run --disableRemoteCache some-feature:lint",
    "some-feature:test": "nx run --disableRemoteCache some-feature:test",
    "some-feature:typecheck": "nx run --disableRemoteCache some-feature:typecheck",
    "prepare": "node -e \"if (process.env.NODE_ENV === 'production' || process.env.CI === 'true') process.exit(0); else process.exit(1);\" || husky"
  },
  "dependencies": {
    "@angular/common": "20.3.0",
    "@angular/compiler": "20.3.0",
    "@angular/core": "20.3.0",
    "@angular/forms": "20.3.0",
    "@angular/platform-browser": "20.3.0",
    "@angular/router": "20.3.0",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "1.21.0",
    "@analogjs/vitest-angular": "1.21.0",
    "@angular-devkit/build-angular": "20.3.1",
    "@angular-devkit/core": "20.3.1",
    "@angular-devkit/schematics": "20.3.1",
    "@angular/build": "20.3.1",
    "@angular/cli": "20.3.1",
    "@angular/compiler-cli": "20.3.0",
    "@angular/language-service": "20.3.0",
    "@eslint/js": "9.35.0",
    "@nx/angular": "21.5.2",
    "@nx/angular-rspack": "21.5.2",
    "@nx/devkit": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/playwright": "21.5.2",
    "@nx/rspack": "21.5.2",
    "@nx/vite": "21.5.2",
    "@nx/web": "21.5.2",
    "@nx/workspace": "21.5.2",
    "@playwright/test": "1.55.0",
    "@rspack/cli": "1.5.3",
    "@rspack/core": "1.5.3",
    "@rspack/dev-server": "1.1.4",
    "@schematics/angular": "20.3.1",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.13.5",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/cli": "4.1.13",
    "@types/node": "20.19.9",
    "@typescript-eslint/utils": "8.43.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "angular-eslint": "20.2.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-playwright": "2.2.2",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "jsdom": "27.0.0",
    "lint-staged": "16.1.6",
    "nx": "21.5.2",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "4.1.13",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "vite": "7.1.5",
    "vitest": "3.2.4",
    "webpack-merge": "6.0.1"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
